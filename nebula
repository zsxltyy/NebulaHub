    textBox.PlaceholderColor3 = Colors.TextSecondary
    textBox.TextSize = 18
    textBox.ClearTextOnFocus = false
    textBox.ZIndex = 113
    textBox.Parent = inputFrame

    local counter = Instance.new("TextLabel")
    counter.Name = "Counter"
